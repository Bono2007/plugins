{
  "name": "np.plugins",
  "main": "lib/script.src.js",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-flow": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.2",
    "flow-bin": "^0.151.0",
    "inquirer": "^8.1.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.0",
    "rollup": "^2.48.0",
    "simple-input": "^1.0.1"
  },
  "scripts": {
    "build:nmn.sweep": "rollup -c ./nmn.sweep/rollup.config.js",
    "build:nmn.Templates": "rollup -c ./nmn.Templates/rollup.config.js",
    "build:np.statistics": "rollup -c ./np.statistics/rollup.config.js",
    "build:jgclark.DailyJournal": "rollup -c ./jgclark.DailyJournal/rollup.config.js",
    "build:jgclark.Filer": "rollup -c ./jgclark.Filer/rollup.config.js",
    "build:jgclark.RepeatExtensions": "rollup -c ./jgclark.RepeatExtensions/rollup.config.js",
    "build:jgclark.CalendarHelpers": "rollup -c ./jgclark.CalendarHelpers/rollup.config.js",
    "build:jgclark.QuickCapture": "rollup -c ./jgclark.QuickCapture/rollup.config.js",
    "build": "concurrently \"npm:build:*\"",
    "watch:nmn.sweep": "rollup -c ./nmn.sweep/rollup.config.js --watch",
    "watch:nmn.Templates": "rollup -c ./nmn.Templates/rollup.config.js --watch",
    "watch:np.statistics": "rollup -c ./np.statistics/rollup.config.js --watch",
    "watch:jgclark.DailyJournal": "rollup -c ./jgclark.DailyJournal/rollup.config.js --watch",
    "watch:jgclark.Filer": "rollup -c ./jgclark.Filer/rollup.config.js --watch",
    "watch:jgclark.RepeatExtensions": "rollup -c ./jgclark.RepeatExtensions/rollup.config.js --watch",
    "watch:jgclark.CalendarHelpers": "rollup -c ./jgclark.CalendarHelpers/rollup.config.js --watch",
    "watch:jgclark.QuickCapture": "rollup -c ./jgclark.QuickCapture/rollup.config.js --watch",
    "watch": "concurrently \"npm:watch:*\"",
    "autowatch": "node scripts/rollup.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier -w ./**/*.js ./**/**/*.js ./**/**/**/*.js",
    "typecheck": "flow check",
    "fix": "npm run lint-fix && npm run format",
    "test": "npm run lint && npm run typecheck"
  },
  "prettier": {
    "printWidth": 80,
    "parser": "flow",
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "enquirer": "^2.3.6",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.0",
    "toml": "^3.0.0"
  }
}
