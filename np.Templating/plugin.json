{
  "macOS.minVersion": "10.13.0",
  "noteplan.minAppVersion": "3.4.1",
  "plugin.id": "np.Templating",
  "plugin.name": " ðŸ“’ np.Templating",
  "plugin.version": "1.0.0-beta.10",
  "plugin.description": "Templating Plugin for NotePlan",
  "plugin.author": "Mike Erickson (@codedungeon)",
  "plugin.dependencies": [],
  "plugin.script": "script.js",
  "plugin.url.old": "https://github.com/NotePlan/plugins/blob/main/np.Templating/README.md",
  "plugin.url": "https://nptemplating-docs.netlify.app/",
  "plugin.commands": [
    {
      "name": "np:init",
      "alias": [
        "npinit"
      ],
      "description": "Initialize np.Templating Configuration",
      "jsFunction": "templateInit"
    },
    {
      "name": "np:insert",
      "alias": [
        "insert"
      ],
      "description": "Insert template to current note",
      "jsFunction": "templateInsert"
    },
    {
      "name": "np:append",
      "alias": [
        "append"
      ],
      "description": "Append template to end of current note",
      "jsFunction": "templateAppend"
    },
    {
      "name": "np:new",
      "alias": [
        "new"
      ],
      "description": "Create new note with title, in chosen folder with optional template",
      "jsFunction": "templateNew"
    },
    {
      "name": "np:advice",
      "alias": [
        "advice"
      ],
      "description": "Returns random advice",
      "jsFunction": "templateAdvice"
    },
    {
      "name": "np:affirmation",
      "alias": [
        "affirmation"
      ],
      "description": "Returns random affirmation",
      "jsFunction": "templateAffirmation"
    },
    {
      "name": "np:quote",
      "alias": [
        "quote"
      ],
      "description": "Returns random quote",
      "jsFunction": "templateQuote"
    },
    {
      "name": "np:verse",
      "alias": [
        "verse"
      ],
      "description": "Returns random bible verse",
      "jsFunction": "templateVerse"
    },
    {
      "name": "np:weather",
      "alias": [
        "weather"
      ],
      "description": "Returns weather for current location",
      "jsFunction": "templateWeather"
    },
    {
      "name": "np:update",
      "alias": [],
      "description": "Run np.Templating Setting",
      "jsFunction": "onUpdateOrInstall"
    },
    {
      "name": "np:migrate-templates",
      "alias": [],
      "description": "Run Template Migration",
      "jsFunction": "migrateTemplates"
    },
    {
      "name": "np:migrate-quick-notes",
      "alias": [],
      "description": "Migration _configuration quickNotes",
      "jsFunction": "migrateQuickNotes"
    },
    {
      "name": "np:qtn",
      "alias": [
        "Quick template note",
        "quick",
        "template",
        "qnt",
        "qtn"
      ],
      "description": "Create new note based on Quick Note Template",
      "jsFunction": "templateQuickNote"
    }
  ],
  "plugin.settings": [
    {
      "type": "heading",
      "title": "np.Templating Settings"
    },
    {
      "key": "version",
      "type": "hidden",
      "title": "np.Templating Settings Version"
    },
    {
      "key": "templateLocale",
      "title": "Template Locale",
      "description": "Locale used by np.Templating Modules (leave blank for system locale)\n\nDefault: <system>",
      "type": "string",
      "default": "<system>",
      "required": false
    },
    {
      "type": "separator"
    },
    {
      "key": "userFirstName",
      "title": "First Name",
      "description": "Used when referencing <%= user.first %>",
      "type": "string",
      "default": "John",
      "required": false
    },
    {
      "key": "userLastName",
      "title": "Last Name",
      "description": "Used when referencing <%= user.last %>",
      "type": "string",
      "default": "Doe",
      "required": false
    },
    {
      "key": "userEmail",
      "title": "Email",
      "description": "Used when referencing <%= email %>",
      "type": "string",
      "default": "name@domain.com",
      "required": false
    },
    {
      "key": "userPhone",
      "title": "Phone",
      "description": "Used when referencing <%= phone %>",
      "type": "string",
      "default": "(714) 555-1212",
      "required": false
    },
    {
      "type": "separator"
    },
    {
      "key": "dateFormat",
      "title": "Date Format",
      "description": "Default date format (may be overridden in desired DateModule method)\n\nDefault: short",
      "type": "string",
      "default": "short",
      "required": false
    },
    {
      "key": "timeFormat",
      "title": "Time Format",
      "description": "Default time format (may be overridden in desired TimeModule method)\n\nDefault: short",
      "type": "string",
      "default": "short",
      "required": false
    },
    {
      "key": "timestampFormat",
      "title": "Timestamp Format",
      "description": "Default format when using date.timestamp()\n\nDefault: YYYY-MM-DD h:mm A",
      "type": "string",
      "default": "YYYY-MM-DD h:mm A",
      "required": false
    },
    {
      "type": "separator"
    },
    {
      "key": "weatherLocation",
      "title": "Weather Lookup Location",
      "description": "Desired location where weather lookup will be used\n\nNote: current location is empty\n\nUse following command to display weather in template\n\n<%- weather(location?:string, url?.string, format?.string) %>",
      "type": "string",
      "default": "",
      "required": false
    },
    {
      "key": "weatherURL",
      "title": "Weather Lookup URL",
      "description": "Weather URL\n\nDefault: https://wttr.in",
      "type": "string",
      "default": "wttr.in",
      "required": false
    },
    {
      "key": "weatherFormat",
      "title": "Weather Lookup Format",
      "description": "Weather Format (uses https://wttr.in)\n\nDefault: format=3",
      "type": "string",
      "default": "format=3",
      "required": false
    },
    {
      "type": "separator"
    },
    {
      "key": "quickNotesFolder",
      "title": "Quick Notes Folder",
      "description": "Path to location of Quick Notes folder in NotePlan @Templates folder\n\nDefault: ðŸ—’ Quick Notes",
      "type": "string",
      "default": "ðŸ—’ Quick Notes",
      "required": false
    },
    {
      "type": "separator"
    },
    {
      "key": "services",
      "title": "Web Services",
      "description": "Configuration for Services which can be referenced using\n<% web.service() %> method\n\nTip: Use https://jsonformatter.org/json5-validator to validate",
      "type": "json",
      "default": "",
      "required": false
    }
  ]
}
